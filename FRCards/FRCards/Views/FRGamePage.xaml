<?xml version="1.0" encoding="utf-8" ?>
<ContentPage xmlns="http://xamarin.com/schemas/2014/forms"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             xmlns:local="clr-namespace:FRCards.Views"
             x:Class="FRCards.Views.FRGamePage">
    <ContentPage.Content>
        <StackLayout>
            <local:DeckSetView BindingContext="{Binding RouleurSet}" Rider="Rouleur" />
            <local:DeckSetView BindingContext="{Binding SprinteurSet}" Rider="Sprinteur" />
        </StackLayout>
    </ContentPage.Content>
</ContentPage>